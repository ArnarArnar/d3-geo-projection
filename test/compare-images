#!/bin/bash

for i in \
    airy \
    aitoff \
    armadillo \
    august \
    baker \
    berghaus \
    boggs \
    bonne \
    bottomley \
    bromley \
    chamberlin \
    collignon \
    craig \
    craster \
    cylindricalEqualArea \
    cylindricalStereographic \
    eckert1 \
    eckert2 \
    eckert3 \
    eckert4 \
    eckert5 \
    eckert6 \
    eisenlohr \
    fahey \
    gilbert \
    gingery \
    ginzburg4 \
    ginzburg5 \
    ginzburg6 \
    ginzburg8 \
    ginzburg9 \
    gringorten \
    mollweide \
    sinusoidal \
    winkel3 \
    ; do
  test/render-world $i \
      && [ "$(compare -metric rmse img/$i.png test/output/$i.png null: 2>&1)" = "0 (0)" ] \
      || echo "âœ— test/output/$i.png and img/$i.png differ"
done
